{
  "name": "mypollingapp-docker",
  "version": "2.0.0",
  "description": "Polling App with Voice Chat, AI Assistant, and Admin Panel - Dockerized",
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "scripts": {
    "dev": "next dev -p 8600",
    "build": "npx prisma generate && next build",
    "start": "next start -p 8600",
    "db:push": "npx prisma db push",
    "db:seed": "npx ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down"
  },
  "dependencies": {
    "@huggingface/inference": "^4.0.0",
    "@prisma/client": "^5.0.0",
    "@tailwindcss/postcss": "^4.0.0",
    "@paypal/checkout-server-sdk": "^1.0.3",
    "authorizenet": "^1.0.10",
    "bcryptjs": "^2.4.3",
    "braintree": "^3.35.0",
    "formidable": "^3.5.0",
    "next": "^14.0.0",
    "next-auth": "^4.24.0",
    "openai": "^4.0.0",
    "qrcode": "^1.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "square": "^38.2.0",
    "stripe": "^17.7.0",
    "swr": "^2.3.8",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/bcryptjs": "^2.4.0",
    "@types/braintree": "^3.3.0",
    "@types/formidable": "^3.4.0",
    "@types/node": "^20.0.0",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^18.2.0",
    "@types/uuid": "^9.0.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    "prisma": "^5.0.0",
    "tailwindcss": "^3.4.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  }
}
